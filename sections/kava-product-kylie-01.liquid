    },
    {
      "type": "checkbox",
      "id": "section_height",
      "default": false,
      "label": "Full screen height"
    },
    {
      "type": "checkbox",
      "id": "inherit_fonts",
      "default": false,
      "label": "Inherit font sizes from theme"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text color",
      "default": "#262424"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background color",
      "default": "transparent"
    },
    {
      "type": "color",
      "id": "accent_color",
      "label": "Accent color",
      "default": "#ED6F43"
    },
    {
      "type": "color",
      "id": "accent_color_text",
      "label": "Text on accent background",
      "default": "#ffffff"
    },
    {
      "type": "select",
      "id": "aspect_ratio",
      "label": "Image ratio",
      "options": [
        {
          "value": "100%",
          "label": "Square"
        },
        {
          "value": "150%",
          "label": "Portrait"
        },
        {
          "value": "75%",
          "label": "4:3"
        }
      ],
      "default": "100%"
    },
    {
      "type": "range",
      "id": "products_limit",
      "min": 1,
      "max": 24,
      "step": 1,
      "unit": "prd",
      "label": "Number of products from collection (24 max)",
      "default": 6
    },
    {
      "type": "range",
      "id": "count",
      "min": 1,
      "max": 4,
      "step": 1,
      "unit": "sld",
      "label": "Slides per page",
      "default": 4
    },
    {
      "type": "range",
      "id": "gutter",
      "min": 0,
      "max": 50,
      "step": 1,
      "unit": "px",
      "label": "Space beеtween slides",
      "default": 30
    },
    {
      "type": "checkbox",
      "id": "controls",
      "default": true,
      "label": "Show controls"
    },
    {
      "type": "range",
      "id": "delay",
      "min": 500,
      "max": 9500,
      "step": 500,
      "unit": "ms",
      "label": "Autoplay speed",
      "default": 5000
    },
    {
      "type": "checkbox",
      "id": "autoplay",
      "default": false,
      "label": "Autoplay"
    },
    {
      "type": "checkbox",
      "id": "loop",
      "default": true,
      "label": "Loop"
    },
    {
      "type": "header",
      "content": "Texts & localization"
    },
    {
      "type": "text",
      "id": "tax_info",
      "label": "Tax info text"
    },
    {
      "type": "text",
      "id": "button_text",
      "label": "Buy now button text",
      "default": "Buy now",
      "info": "Shown when product is available and has one variant"
    },
    {
      "type": "text",
      "id": "sold_out_text",
